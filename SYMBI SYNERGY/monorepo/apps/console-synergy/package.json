{
  "name": "console-synergy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dev:api": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "next lint",
    "start": "next start",
    "start:test": "NODE_ENV=test npm start",
    "test": "npm run test:unit && npm run test:api && npm run test:e2e",
    "test:unit": "jest --config jest.config.js --testPathPattern=tests/unit",
    "test:api": "playwright test --config playwright.config.ts --testDir tests/api",
    "test:e2e": "playwright test --config playwright.config.ts --testDir tests/e2e",
    "test:visual": "playwright test --config playwright.config.ts --testDir tests/visual",
    "test:performance": "playwright test --config playwright.config.ts --testDir tests/performance",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "db:seed:test": "node scripts/seed-test-db.js",
    "db:seed:performance": "node scripts/seed-performance-db.js",
    "db:clean:test": "node scripts/clean-test-db.js",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  },
