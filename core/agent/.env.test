# Test Environment Configuration
NODE_ENV=test
LOG_LEVEL=silent

# Mock services
MOCK_EXTERNAL_SERVICES=true
ORCHESTRATOR_URL=http://localhost:3000
AGENT_API_KEY=test-api-key

# Test timeouts (shorter for faster tests)
REQUEST_TIMEOUT=5000
HEALTH_CHECK_INTERVAL=1000
METRICS_INTERVAL=1000

# Test database (if using one)
DATABASE_URL=sqlite://test.db

# Disable external integrations in tests
WEBHOOK_URL=http://localhost:3001/test-webhook
REPOSITORY_URL=https://github.com/test/test-repo
REPOSITORY_TOKEN=test-token
WEBSITE_DOMAIN=test.example.com

# Test-specific settings
SUPPRESS_CONSOLE=true
RATE_LIMIT_REQUESTS=1000
RATE_LIMIT_WINDOW=1000