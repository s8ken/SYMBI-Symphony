{
  "name": "@yseeku/trust-protocol",
  "version": "0.1.0",
  "description": "W3C-compliant trust infrastructure for AI agents. Decentralized identity (DID), verifiable credentials (VC), privacy-preserving revocation, and cryptographic audit trails.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/s8ken/SYMBI-Symphony.git"
  },
  "bugs": {
    "url": "https://github.com/s8ken/SYMBI-Symphony/issues"
  },
  "homepage": "https://symbi.world/symbi-symphony",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:trust": "jest --testPathPattern=trust --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:merge": "node scripts/merge-coverage.js",
    "test:vectors": "jest --testPathPattern=vectors",
    "test:interop": "jest --testPathPattern=interop",
    "test:properties": "jest --testPathPattern=properties",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "security:scan": "npm audit && snyk test",
    "docs:api": "typedoc --out docs/api src/core",
    "audit:integrity": "tsx scripts/audit-integrity.ts",
    "test:security": "npm audit --audit-level=moderate && echo 'Security check passed'",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "pilot:enroll": "node dist/scripts/pilot/enroll.js",
    "pilot:issue": "node dist/scripts/pilot/issue.js",
    "pilot:verify": "node dist/scripts/pilot/verify.js",
    "pilot:revoke": "node dist/scripts/pilot/revoke.js"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "agents",
    "trust",
    "decentralized-identity",
    "did",
    "verifiable-credentials",
    "w3c",
    "revocation",
    "audit-trail",
    "cryptography",
    "eu-ai-act",
    "compliance",
    "symbi",
    "symphony"
  ],
  "author": "SYMBI Team <stephen@yseeku.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-security": "^2.1.0",
    "fast-check": "^4.3.0",
    "jest": "^29.0.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.1.0",
    "snyk": "^1.1250.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0",
    "ws": "^8.18.3"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.490.0",
    "@ethersproject/contracts": "^5.8.0",
    "@ethersproject/providers": "^5.8.0",
    "@google-cloud/kms": "^4.2.0",
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1",
    "@noble/secp256k1": "^3.0.0",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "fs-extra": "^11.0.0",
    "ioredis": "^5.3.2",
    "json-canonicalize": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "winston": "^3.11.0"
  }
}
