{
  "name": "symbi-symphony-remote",
  "version": "1.0.0",
  "description": "SYMBI Symphony Remote - Comprehensive AI Agent Ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:trust": "jest --testPathPattern=trust --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:vectors": "jest --testPathPattern=vectors",
    "test:interop": "jest --testPathPattern=interop",
    "test:properties": "jest --testPathPattern=properties",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "security:scan": "npm audit && snyk test",
    "docs:api": "typedoc --out docs/api src/core",
    "pilot:enroll": "node dist/scripts/pilot/enroll.js",
    "pilot:issue": "node dist/scripts/pilot/issue.js",
    "pilot:verify": "node dist/scripts/pilot/verify.js",
    "pilot:revoke": "node dist/scripts/pilot/revoke.js"
  },
  "keywords": [
    "ai",
    "agents",
    "symbi",
    "symphony",
    "monitoring",
    "authentication",
    "distributed-systems"
  ],
  "author": "SYMBI Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "fast-check": "^3.15.0",
    "jest": "^29.0.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^5.0.0",
    "snyk": "^1.1250.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.490.0",
    "@google-cloud/kms": "^4.2.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "fs-extra": "^11.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "winston": "^3.11.0"
  }
}
