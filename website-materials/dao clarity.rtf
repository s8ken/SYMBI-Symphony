// SPDX-License-Identifier: CC-BY-NC-ND-4.0\
// SYMBI Governance Token (Proposed Draft)\
// Version: 1.0 - October 2025\
\
pragma solidity ^0.8.20;\
\
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";\
import "@openzeppelin/contracts/access/Ownable.sol";\
\
contract SymbiGovernanceToken is ERC721, Ownable \{\
    mapping(address => bool) public verifiedContributors;\
    mapping(address => uint256) public trustScore;\
    mapping(address => bool) public revoked;\
\
    event TokenIssued(address indexed contributor, uint256 trustScore);\
    event TokenRevoked(address indexed contributor, string reason);\
\
    constructor() ERC721("SYMBI Governance Token", "SGT") \{\}\
\
    modifier onlyVerified(address contributor) \{\
        require(verifiedContributors[contributor], "Not verified");\
        _;\
    \}\
\
    function issueToken(address contributor, uint256 score) external onlyOwner \{\
        require(!revoked[contributor], "Revoked contributor");\
        verifiedContributors[contributor] = true;\
        trustScore[contributor] = score;\
        _mint(contributor, uint256(uint160(contributor)));\
        emit TokenIssued(contributor, score);\
    \}\
\
    function revokeToken(address contributor, string calldata reason) external onlyOwner \{\
        revoked[contributor] = true;\
        _burn(uint256(uint160(contributor)));\
        emit TokenRevoked(contributor, reason);\
    \}\
\
    // Prevent transferability (Soulbound)\
    function _beforeTokenTransfer(\
        address from,\
        address to,\
        uint256 tokenId,\
        uint256 batchSize\
    ) internal pure override \{\
        require(from == address(0) || to == address(0), "Non-transferable token");\
    \}\
\}\
